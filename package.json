{
  "name": "landing-mavic",
  "version": "1.0.0",
  "description": "Landing page for Mavic 2 Pro",
  "private": true,
  "scripts": {
    "start": "webpack serve --config webpack/webpack.dev.js",
    "build": "npm run clean & SET NODE_ENV=production & webpack --config webpack/webpack.prod.js",
    "clean": "rimraf dist",
    "tscheck": "tsc",
    "tscheck:watch": "tsc -w"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "last 1 year",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "license": "MIT",
